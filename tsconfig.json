{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "moduleResolution": "Node",
  "jsx": "react-jsx",
  "strict": false,
  "noImplicitAny": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "noEmitOnError": false,
    "types": [
      "react",
      "react-dom"
    ],
    "baseUrl": ".",
    "paths": {
      "react-konva": [ "./shims/reactKonva" ],
      "react/jsx-runtime": [ "./shims/jsxRuntime" ],
      "its-fine": [ "./shims/itsFine" ],
      "@atoms/*": [ "./ui/atoms/*" ],
      "@molecules/*": [ "./ui/molecules/*" ],
      "@organisms/*": [ "./ui/organisms/*" ],
      "@templates/*": [ "./ui/templates/*" ],
      "@pages/*": [ "./ui/pages/*" ],
      "@hooks/*": [ "./hooks/*" ],
      "@contexts/*": [ "./contexts/*" ],
      "@utils/*": [ "./utils/*" ],
      "@types/*": [ "./types/*" ],
      "@expo": [ "../../../../*" ],
      "@tinyartist/*": [ "../../../*" ],
      "@editor": [ "./*" ],
      "@assets": [ "./assets/*" ],
    }
  },
  "include": [
    ".",
    "utils",
    "hooks",
    "ui",
    "ui/organisms/canvas",
    "templates",
    "global.d.ts",
    "index.tsx"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
