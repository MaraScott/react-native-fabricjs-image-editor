{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "noEmitOnError": false,
    "types": [
      "react",
      "react-dom"
    ],
    "baseUrl": ".",
    "paths": {
      "react-konva": [
        "./shims/reactKonva"
      ],
      "react/jsx-runtime": [
        "./shims/jsxRuntime"
      ],
      "its-fine": [
        "./shims/itsFine"
      ],
      "@atoms/*": [
        "./ui/atoms/*"
      ],
      "@molecules/*": [
        "./ui/molecules/*"
      ],
      "@organisms/*": [
        "./ui/organisms/*"
      ],
      "@templates/*": [
        "./ui/templates/*"
      ],
      "@pages/*": [
        "./ui/pages/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@contexts/*": [
        "./contexts/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@types/*": [
        "./types/*"
      ],
      "@canvas/*": [
        "./canvas/*"
      ],
      "@vendor/*": [
        "./vendor/*"
      ],
      "@editor": [
        "./*"
      ],
      "@expo": [
        "../../../../*"
      ],
      "@tinyartist/*": [
        "../../../*"
      ]
    }
  },
  "include": [
    ".",
    "utils",
    "hooks",
    "ui",
    "canvas",
    "templates",
    "global.d.ts",
    "index.tsx"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
