{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "moduleResolution": "Node",
  "jsx": "react-jsx",
  "strict": false,
  "noImplicitAny": false,
  "allowJs": false,
  "checkJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "noEmitOnError": false,
    "types": [
      "react",
      "react-dom"
    ],
    "baseUrl": ".",
    "paths": {
      "react-konva": [ "./shims/reactKonva" ],
      "react/jsx-runtime": [ "./shims/jsxRuntime" ],
      "its-fine": [ "./shims/itsFine" ],
      "redux": [ "./shims/redux" ],
      "react-redux": [ "./shims/react-redux" ],
      "redux-thunk": [ "./shims/redux-thunk" ],
      "reduxThunk": [ "./shims/reduxThunk" ],
      "reduxToolkit": [ "./shims/reduxToolkit" ],
      "@reduxjs/toolkit": [ "./shims/reduxjs/toolkit" ],
      "@atoms/*": [ "./ui/atoms/*" ],
      "@molecules/*": [ "./ui/molecules/*" ],
      "@organisms/*": [ "./ui/organisms/*" ],
      "@templates/*": [ "./ui/templates/*" ],
      "@pages/*": [ "./ui/pages/*" ],
      "@hooks/*": [ "./hooks/*" ],
      "@contexts/*": [ "./contexts/*" ],
      "@utils/*": [ "./utils/*" ],
      "@types/*": [ "./types/*" ],
      "@store/*": [ "./store/*" ],
      "store/*": [ "./store/*" ],
      "@expo": [ "../../../../*" ],
      "@tinyartist/*": [ "../../../*" ],
      "@editor": [ "./*" ],
      "@assets": [ "./assets/*" ],
    }
  },
  "include": [
    ".",
    "utils",
    "hooks",
    "ui",
    "ui/organisms/canvas",
    "templates",
    "global.d.ts",
    "index.tsx"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "scripts"
  ]
}
